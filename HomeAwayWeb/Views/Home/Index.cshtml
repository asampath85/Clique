<h3 style="margin-top: 0px;">
    Dashboard
</h3>
<label>Summary of list of properties that are available in Aribnb. </label>
<br />
<br />
<div class="row" style="height: 380px;">
    <div class="col-md-12">

        <table id="example" class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Property Name</th>
                    <th>Property Type</th>
                    <th>City</th>
                    <th>One Day Price</th>
                    <th>One Week Price</th>
                </tr>
            </thead>
            <tbody>
                <!-- ko foreach: propertyList() -->
                <tr>
                    <td>
                        <a data-bind="text: Id, click: $root.navigatetoDetails" target="_blank"></a>
                    </td>
                    <td data-bind="text: Name"></td>
                    <td data-bind="text: Type"></td>
                    <td data-bind="text: City"></td>
                    <td data-bind="text: NightPrice"></td>
                    <td data-bind="text: WeekPrice"></td>

                </tr>
                <!-- /ko -->
            </tbody>
        </table>

    </div>
</div>

@section scripts{
    <script type="text/javascript">

        $(document).ready(function () {
            var viewModel = new DashboardViewModel();
            ko.applyBindings(viewModel);


        });
    </script>
}

