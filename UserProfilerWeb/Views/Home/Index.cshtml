<h1>
    Dashboard
</h1>
<div class="right_b">
    <span>Summary of list of properties that are available in Aribnb. </span>
    <br />
    <br />
</div>
<div class="row">
    <div class="col-md-12">

        <table id="example" class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Property Name</th>
                    <th>City</th>                    
                    <th>From Date</th>
                    <th>To Date</th>
                    <th>Base Price</th>
                    <th>Score</th>
                    <th>Status</th>
                </tr>
            </thead>
            <tbody>
                <!-- ko foreach: requestList() -->
                <tr>
                    <td data-bind="text: Id"></td>
                    <td data-bind="text: Name"></td>
                    <td data-bind="text: City"></td>
                    <td data-bind="text: FromDate"></td>
                    <td data-bind="text: ToDate"></td>
                    <td data-bind="text: Price"></td>
                    <td data-bind="text: Score"></td>
                    <td>
                        <a data-bind="text: StatusName, click: $root.navigatetoDetails" target="_blank"></a>
                    </td>
                    @*<td data-bind="text: StatusName, click: $root.navigatetoDetails" style="cursor:hand;"></td>*@

                </tr>
                <!-- /ko -->
            </tbody>
        </table>

    </div>
</div>

@section scripts{

  
    <script type="text/javascript">

        $(document).ready(function () {
            var viewModel = new DashboardViewModel();
            ko.applyBindings(viewModel);


        });
    </script>
}

